#: src/collectionfieldsdialog.cpp:148
#, kde-format
msgid "Field Properties"
msgstr "Alan Özellikleri"

#: src/collectionfieldsdialog.cpp:157
#, kde-format
msgid "&Title:"
msgstr "&Başlık:"

#: src/collectionfieldsdialog.cpp:162
#, kde-format
