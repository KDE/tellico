[KNewStuff2]
#ProvidersUrl=http://periapsis.org/tellico/newstuff/tellicotemplates-providers.php
ProvidersUrl=http://download.kde.org/khotnewstuff/tellicotemplates-providers.xml
ChecksumPolicy=ifpossible
SignaturePolicy=ifpossible
TargetDir=tellico/tmp
Uncompress=never
InstallationCommand=dbus-send --type=method_call --dest=org.tellico_project /newstuff org.tellico_project.newstuff.installTemplate string:%f
UninstallCommand=dbus-send --type=method_call --dest=org.tellico_project /newstuff org.tellico_project.newstuff.removeTemplate string:%f
